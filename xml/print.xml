<program>
  <print>
    <assign to="x">
      <integer>29</integer>
    </assign>
  </print>

  <print>
    <var>x</var>
  </print>

  <print>
    <integer>
      8
    </integer>
  </print>

  <integer>3</integer>

  <print>
    <integer>
      200
    </integer>
  </print>

  <assign to="a">
    <vector>
      <row>
        <integer>1</integer>
        <integer>1</integer>
        <integer>3</integer>
        <integer>2</integer>
      </row>
      <row>
        <integer>5</integer>
        <integer>2</integer>
        <integer>2</integer>
        <integer>3</integer>
      </row>
    </vector>
  </assign>

  <print>
    <var>a</var>
  </print>

  <print>
    <transpose>
      <var>a</var>
    </transpose>
  </print>

  <assign to="w">
    <row>
      <integer>1</integer>
      <integer>2</integer>
      <integer>3</integer>
    </row>
  </assign>

  <assign to="v">
    <transpose>
      <var>w</var>
    </transpose>
  </assign>

  <print>
    <var>w</var>
  </print>

  <print>
    <var>v</var>
  </print>

  <print>
    <dot>
      <var>w</var>
      <var>v</var>
    </dot>
  </print>

  <print>
    <assign to="m">
      <random rows="3" cols="3" low="1" high="4"/>
    </assign>
  </print>

  <print>
    <div>
      <var>m</var>
      <var>m</var>
    </div>
  </print>

  <print>
    <div>
      <var>m</var>
      <integer>1</integer>
    </div>
  </print>

  <print>
    <div>
      <integer>-1</integer>
      <integer>11</integer>
    </div>
  </print>

</program>